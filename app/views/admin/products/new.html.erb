<h1>syouhinntuikka</h1>
<%= form_for @product, url: new_admin_product_path do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>商品名</h4>
  <%= f.text_field :name %>
  <h4>商品価格</h4>
  <%= f.text_field :price %>
  <h4>商品説明</h4>
  <%= f.text_area :introduction %>
  <%= f.submit '投稿' %>
<% end %>